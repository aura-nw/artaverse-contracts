{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "base_token_uri",
    "cw721_code_id",
    "max_tokens_per_batch_mint",
    "name",
    "num_tokens",
    "symbol"
  ],
  "properties": {
    "base_token_uri": {
      "type": "string"
    },
    "cw721_code_id": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "max_tokens_per_batch_mint": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "name": {
      "type": "string"
    },
    "num_tokens": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "royalty_payment_address": {
      "type": [
        "string",
        "null"
      ]
    },
    "royalty_percentage": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint64",
      "minimum": 0.0
    },
    "symbol": {
      "type": "string"
    }
  }
}
